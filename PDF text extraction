# ====================================================================================================# Extraction at PAGE_V2 level# ====================================================================================================def parse_page_v2(path: pathlib.Path, granularity: Granularity) -> List[Content]:    """    Parsing a docx by paragraph    Args:        page_dict:        granularity:    Returns:    """    # Defining the output structure    contents: List[Content] = []    with pdfplumber.open(str(path)) as pdf:        for page_number, page in enumerate(pdf.pages, start=1):            page_text = page.extract_text()            contents.append(                Content(                    text=page_text,                    page_number=page_number,                    granularity=granularity,                    item_number=page_number,                    vector=None,                    parser=get_parent_func_name()                )            )    return contents
 
